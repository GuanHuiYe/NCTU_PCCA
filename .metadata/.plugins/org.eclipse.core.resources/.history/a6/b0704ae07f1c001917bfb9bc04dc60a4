package Day1;
import java.util.*;

public class B_Ehab_and_subtraction {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String[] input=sc.nextLine().split(" ");		
		String[] input2=sc.nextLine().split(" ");
		int[] intarr=new int[Integer.parseInt(input[0])];
		for(int n=0;n<Integer.parseInt(input[0]);n++) {
			intarr[n]=Integer.parseInt(input2[n]);
		}
		Arrays.sort(intarr);
	    int sumansarr=0;
	    int nth=0;
	    int n=0;
	    while(nth<Integer.parseInt(input[1])) {
	    	int num=intarr[n];
	    	if(n==0)System.out.println(num);
	    	else {
	    		if(n<Integer.parseInt(input[0])) {
	    			for(int nn=0;nn<nth;nn++) {
		    			num-=ansarr[nn];
		    			if(num<=0)break;
		    		}
		    		if(num<=0) {
		    			n++;
		    			continue;
		    		} 
		    		System.out.println(num);
	    			
	    		}
	    		else {
	    			num=0;
	    		}
	    		
	    	}
	    	ansarr[nth]=num;
	    	nth++;
	    	
	    }
		

	}

}
